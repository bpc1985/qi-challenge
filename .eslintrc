{
  "rules": {
    "angular/controller-as-route": 0,
    "angular/di": 0,
    "angular/interval-service": 0,
    "angular/module-setter": 0,
    "angular/no-private-call": 0,
    "angular/timeout-service": 0,
    "angular/no-http-callback": 0,
    "arrow-body-style": 0,
    "brace-style": 0,
    "camelcase": 0,
    "comma-dangle": 0,
    "comma-spacing": 0,
    "consistent-return": 0,
    "eol-last": 0,
    "global-require": 0,
    "key-spacing": 0,
    "keyword-spacing": 0,
    "max-len": 0,
    "new-cap": 0,
    "newline-per-chained-call": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-control-regex": 0,
    "no-else-return": 0,
    "no-extra-semi": 0,
    "no-multi-spaces": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-redeclare": 0,
    "no-return-assign": 0,
    "no-shadow": 0,
    "no-trailing-spaces": 0,
    "no-unreachable": 0,
    "no-use-before-define": 0,
    "no-useless-concat": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "one-var": 0,
    "padded-blocks": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-rest-params": 0,
    "prefer-template": 0,
    "quote-props": 0,
    "semi": 0,
    "space-before-blocks": 0,
    "space-before-function-paren": 0,
    "space-in-parens": 0,
    "space-infix-ops": 0,
    "spaced-comment": 0,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "strict": 0,
    "no-unused-vars": 0,
    "quotes": 0,
    "no-undef": 0,
    "no-unused-expressions": 0
  },
  "globals": {
    "$": true,         // jQuery (from NPM but installs a global)
    "angular": true,   // AngularJS (from NPM but installs a global)
    "_": true,         // lodash
    "moment": true
  }
}
